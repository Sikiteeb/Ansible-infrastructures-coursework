apiVersion: 1

datasources:
  - name: Prometheus
    type: prometheus
    access: proxy
    orgId: 1
    url: http://sikiteeb-3:{{ prometheus_port }}/prometheus
    isDefault: true
    version: 1
    editable: true

  - name: InfluxDB
    type: influxdb
    access: proxy
    orgId: 1
    url: http://sikiteeb-3:{{ influxdb_port }}
    database: latency
    version: 1
    editable: true
    
  - name: InfluxDB-telegraf
    type: influxdb
    access: proxy
    orgId: 1
    url: http://sikiteeb-3:{{ influxdb_port }}
    database: telegraf
    version: 1
    editable: true  